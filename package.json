{
  "name": "base",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/node": "^18.7.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "antd": "^4.22.4",
    "axios": "^0.27.2",
    "moment": "^2.29.4",
    "npm": "^8.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --testResultsProcessor=jest-sonar-reporter --coverage --reporters=default --reporters=jest-junit --coverageReporters=cobertura --coverageReporters=lcov --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/file-saver": "^2.0.5",
    "@types/styled-components": "^5.1.26",
    "axios-mock-adapter": "^1.21.2",
    "express": "^4.18.1",
    "history": "^5.3.0",
    "jest-junit": "^14.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-trx-results-processor": "^3.0.1",
    "minimatch": "^5.1.0",
    "nth-check": "^2.1.1",
    "nwsapi": "^2.2.1",
    "postcss": "^8.4.16",
    "terser": "^5.14.2",
    "terser-webpack-plugin": "^5.3.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "**/*.d.ts",
      "!**/declarations/*",
      "!**/react-app-env.d.ts",
      "!**/__snapshots__/**"
    ],
    "resetMocks": true,
    "restoreMocks": true
  },
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack",
    "express": "$express",
    "history": "$history",
    "nwsapi": "$nwsapi",
    "postcss": "$postcss",
    "terser-webpack-plugin": "$terser-webpack-plugin",
    "terser": "$terser",
    "nth-check": "$nth-check"
  }
}
